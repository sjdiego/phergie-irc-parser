{
    "abandoned": true,
    "name": "phergie/phergie-irc-parser",
    "description": "Parser for messages conforming to the IRC protocol",
    "keywords": [ "irc", "parser" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "team@phergie.org",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/phergie/phergie-irc-parser/issues",
        "source": "https://github.com/phergie/phergie-irc-parser"
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phergie/phergie-irc-bot-react-development": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
